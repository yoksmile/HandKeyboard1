<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >


    <Row android:keyHeight="150dip"  android:rowEdgeFlags="bottom">
        <Key android:keyWidth="50%p" android:horizontalGap="25%p"
             android:codes="-1" android:keyIcon="@drawable/camera"  
             android:isModifier="true" 
             android:isSticky="true" android:isRepeatable="true" android:rowEdgeFlags="bottom"/>
    </Row>
    
    <Row>
        <Key android:codes="-99" android:keyLabel="Eng" android:keyWidth="30%p" 
             android:keyEdgeFlags="left"/>
        <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" 
                android:keyWidth="40%p" android:isRepeatable="true"/>
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" 
                android:keyWidth="30%p" 
                android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
    
